<!DOCTYPE html>
<html>
<head>
	<title>Carrinho</title>
	<script type="text/javascript">
		function calculo() {
            var qtd= parseInt (document.getElementById('qnt').value);
            var totd= qtd * 25;
            document.getElementById('tot').value = totd;
        }

		
	</script>
</head>
<body>
<div id="carrinho" class="carrinho">
        <form action="#" oninput="calculo();"> 
        <table id="carrinho">
          <tr>
            <td>
              <a href="#">Quantidade total</a>
            </td>
            <td>
              <input type="number" name="qnt" id="qnt">
            </td>
          </tr>
          <tr>
          <td>
            <a href="#">Preco a pagar</a>
          </td>
          <td>
            <input type="text" name="tot" id="tot" readonly="" placeholder="Total a pagar">
          </td>
          </tr>
        </table>
        </form>
        

        </form>
        </table>
    </div>
</body>
</html>
